    <link rel="stylesheet" href="../orders/stylle.css">
    <link href="http://fonts.googleapis.com/css?family=Source+Sans+Pro:400,400italic,600,600italic,700,700italic,900italic,900,300,300italic" rel="stylesheet" type="text/css">
<body>
 
    <section id="order-tracking" class="section-padding-100">
        <div class="container">
            <div class="row">
                <div  class="col-sm-12 col-xs-12 col-md-12">
    <div class="block-content">
        <div class="table-responsive">
            <table class="table table-clean-paddings margin-bottom-0">
                <thead >
                    <tr>
                  <th scope="col">Name & Date</th>
                  <th>Address</th>
                  <th>Pincode</th>
                  <th>Mobile</th>
                  <th scope="col">Amount</th>
                  <th scope="col">Payment</th>
                  <th>Status</th>
                    </tr>
                </thead>
       <tbody>
              {{#each orders}}
              <tr>
                 <td><div style="color:rgb(54, 49, 49);" class="contact-container">{{this.deliveryDetails.name}} <span>{{this.day}}-{{this.month}}-{{this.year}}</span></div></td> 
                <td>{{this.deliveryDetails.address}}</td>
                <td>{{this.deliveryDetails.pincode}}</td>
                <td>{{this.deliveryDetails.mobile}}</td>
                <td>â‚¹{{this.totalAmount}}</td>
                <td>{{this.paymentMethod}}</td>
                <td><span class="label label-primary label-transparent">{{this.status}}</span></td>
              </tr>
            {{/each}}
          </tbody>
            </table>
        </div>
    </div>
   </div>
  </div>
  </div>
</section>

</body>

